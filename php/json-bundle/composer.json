{
    "name": "petrlatsko/jsonbundle",
    "type": "symfony-bundle",
    "description": "Symfony2 JSON bundle with CURL transport",
    "license": "MIT",
    "homepage": "https://github.com/petr-latsko/symfony-json-bundle",
    "keywords" : ["json", "json bundle"],
    "authors" : [{
        "name" : "Petr Latsko",
        "email" : "petr.latsko@gmail.com"
    }],    "autoload": {
    "psr-4": {
        "PetrLatsko\\JsonBundle\\": ""
    }
},
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "curl/curl": "^1.8"
    },
    "config": {
        "platform": {
            "php": "5.3.9"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
